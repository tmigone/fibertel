{
  "name": "fibertel",
  "version": "0.1.11",
  "description": "",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "fibertel": "./bin/run"
  },
  "keywords": [
    "typescript",
    "fibertel",
    "niveles",
    "stats",
    "internet",
    "monitor"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc",
    "bin": "node bin/run",
    "prepare": "npm run build",
    "test": "ava",
    "watch": "nodemon --watch src --ignore tests --ext js,ts,json --exec node -r ts-node/register/transpile-only src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tmigone/fibertel.git"
  },
  "author": "Tom√°s Migone <tomasmigone@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tmigone/fibertel/issues"
  },
  "homepage": "https://github.com/tmigone/fibertel#readme",
  "engines": {
    "node": ">=12.4.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.0",
    "ava": "^3.15.0",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "axios": "^0.21.1",
    "camelcase": "^5.3.1",
    "cheerio": "^1.0.0-rc.9"
  },
  "files": [
    "bin",
    "build"
  ],
  "ava": {
    "files": [
      "test/*",
      "!test/fixtures",
      "!test/lib"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
